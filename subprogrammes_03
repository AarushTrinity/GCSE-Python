def menu():
    print("1) 1-player")
    print("2) 2-player")
    print("3) Online")
    print("4) Exit")

menu()
option = int(input("Choose one of the options"))

if option == 1:
    print("1 player selected")
elif option == 2:
    print("2 player selected")
elif option == 3:
    print("Online selected")
elif option == 4:
    print("Exit...")
